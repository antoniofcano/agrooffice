<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	<record model="ir.ui.view" id="agro_health_tipo_aplicacion_form_view">
            <field name="name">agro.health.tipo.aplicacion.form.view</field>
            <field name="model">agro.health.tipo.aplicacion</field>
            <field name="arch" type="xml">
            	<form string="Tipo de aplicacion">
            		<group colspan="4" col="2">
                                <field name="name"/>
    			</group>
                </form>
            </field>
         </record>                  

        <record id="view_agro_health_tipo_aplicacion" model="ir.ui.view">
            <field name="name">agro.health.tipo.aplicacion.tree</field>
            <field name="model">agro.health.tipo.aplicacion</field>
            <field name="arch" type="xml">
                <tree string="Tipo de aplicaciones">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_agro_health_tipo_aplicacion_search_form" model="ir.ui.view">
            <field name="name">agro.health.tipo.aplicacion.search.form</field>
            <field name="model">agro.health.tipo.aplicacion</field>
            <field name="arch" type="xml">
               <search string="Tipos de aplicaciones">
                    <field name="name"/>
                </search>
            </field>
        </record>

     <!-- ACCION -->
	<record id="act_agro_health_tipo_aplicacion" model="ir.actions.act_window">
	    <field name="name">Tipos de aplicaciones</field>
	    <field name="res_model">agro.health.tipo.aplicacion</field>
	    <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_agro_health_tipo_aplicacion_search_form"/>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to create a new Tipo de aplicacion.
              </p><p>
                OpenERP's agro Tipo de aplicacion  health management...
              </p>
            </field>
	</record>
		

	<!-- MENU -->
	<menuitem id="menu_agro_health_tipo_aplicacion" 
                  name="Tipo de aplicacion"
                  parent="agro_base.menu_agro_config"
                  action="act_agro_health_tipo_aplicacion"/>
	</data>
</openerp>
