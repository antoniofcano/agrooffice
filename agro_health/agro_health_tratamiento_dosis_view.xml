<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	<record model="ir.ui.view" id="agro_health_tratamiento_dosis_form_view">
            <field name="name">agro.health.tratamiento.dosis.form.view</field>
            <field name="model">agro.health.tratamiento.dosis</field>
            <field name="arch" type="xml">
            	<form string="Dosis">
            		<group colspan="4" col="2">
                                <field name="tratamiento_id"/>
                                <field name="product_id"/>
                                <field name="dosis"/>
    			</group>
                </form>
            </field>
         </record>                  

        <record id="view_agro_health_tratamiento_dosis" model="ir.ui.view">
            <field name="name">agro.health.tratamiento.dosis.tree</field>
            <field name="model">agro.health.tratamiento.dosis</field>
            <field name="arch" type="xml">
                <tree string="Dosis">
                                <field name="product_id"/>
                                <field name="dosis"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>
